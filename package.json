{
  "name": "amazon-ads-sdk",
  "version": "1.0.0",
  "description": "Amazon Advertising API SDK for Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "generate": "openapi-generator-cli generate -i ./specs/sponsored-products.json -g typescript-axios -o ./src/generated/sponsored-products --skip-validate-spec",
